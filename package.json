{
  "name": "companydashboard",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "cra-template": "1.2.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-redux": "^9.2.0",
    "react-scripts": "5.0.1",
    "redux": "^5.0.1",
    "web-vitals": "^2.1.4"
  },
"scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "set PORT=4444 && react-scripts start",
    "coverage": "react-scripts test --coverage --watchAll --collectCoverageFrom='src/**/*.{js,jsx,ts,tsx}' --collectCoverageFrom='!src/**/*.{types,stories,contants,test,spec,index}.{ts,tsx,js,jsx}'"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
"browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
"jest": {
    "moduleNameMapper": {
      "^axios$": "axios/dist/node/axios.cjs"
    },
    "coveragePathIgnorePatterns": [
      "node_modules",
      "test-config",
      "interfaces",
      "jestGlobalMocks.ts",
      ".module.ts",
      "<rootDir>/src/reportWebVitals.js",
      "<rootDir>/src/react-app-env.d.js",
      "<rootDir>/src/mocks",
      ".mock.ts"
    ],
    "coverageThreshold": {
      "global": {
        "NODE_TLS_REJECT_UNAUTHORIZED": "0",
        "branches": 75,
        "functions": 80,
        "Lines": 80,
        "statements":80
      }
    }
  },
"devDependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "jest-fetch-mock": "^3.0.3",
    "redux-mock-store": "^1.5.5",
    "redux-saga": "^1.3.0",
    "redux-saga-test-plan": "^4.0.6"
  }
}